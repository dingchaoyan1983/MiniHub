{
  "name": "LM-code-editor",
  "version": "1.0.0",
  "description": "lm code eidtor",
  "main": "index.js",
  "scripts": {
    "start": "node ./bin/www",
    "server": "./node_modules/.bin/webpack-dev-server --config=./webpack.config.js --progress --colors",
    "dev": "./node_modules/.bin/better-npm-run build",
    "watch": "./node_modules/.bin/better-npm-run build:watch",
    "prod": "./node_modules/.bin/better-npm-run build:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:watch": {
      "command": "webpack --watch --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "code editor"
  ],
  "author": "DingChao <270422767@qq.com>",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classname": "^0.0.0",
    "diff-match-patch": "^1.0.0",
    "es6-promise": "^4.0.5",
    "faker": "^3.1.0",
    "immutable": "^3.8.1",
    "jquery": "^3.1.1",
    "normalize.css": "^5.0.0",
    "pretender": "^1.4.2",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.3",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "body-parser": "~1.15.2",
    "consolidate": "^0.14.5",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "mustache": "^2.3.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "node-sass": "^3.10.0",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  }
}
