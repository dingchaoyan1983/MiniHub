{
  "name": "douyu-vue",
  "version": "1.0.0",
  "description": "www.douyu.com vue markup",
  "main": "index.js",
  "scripts": {
    "server": "./node_modules/.bin/webpack-dev-server --config=./webpack.config.js --progress --colors",
    "dev": "./node_modules/.bin/better-npm-run build",
    "watch": "./node_modules/.bin/better-npm-run build:watch",
    "prod": "./node_modules/.bin/better-npm-run build:prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:watch": {
      "command": "webpack --watch --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --progress --colors --display-error-details",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "www.douyu.com",
    "vue",
    "markup",
    "practice"
  ],
  "author": "DingChao <270422767@qq.com>",
  "license": "ISC",
  "dependencies": {
    "classname": "0.0.0",
    "es6-promise": "^4.0.5",
    "vue": "^2.1.8",
    "vue-router": "^2.1.1",
    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.14.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-vue-jsx": "^3.1.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "node-sass": "^3.10.0",
    "postcss-loader": "^0.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^9.7.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  }
}
